<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.app.binary">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/binary"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/binary_red"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".SingleInput"></activity>
        <activity android:name=".SingleInter"></activity>
        <activity android:name=".DoubleInter"/>
    </application>

</manifest>